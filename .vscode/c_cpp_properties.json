{
  "configurations": [
    {
      "name": "gameboy",
      "includePath": [
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/res/**",
        "${workspaceFolder}/../gbdk/include/**"
      ],
      "defines": ["__PORT_gbz80", "__TARGET_gb", "__SDCC", "NINTENDO"],
      "compilerPath": "${workspaceFolder}/../gbdk/bin/lcc",
      "cStandard": "c11",
      "intelliSenseMode": "${default}",
      "compilerArgs": [],
      "browse": {
        "limitSymbolsToIncludedHeaders": true
      },
      "configurationProvider": "ms-vscode.makefile-tools"
    }
  ],
  "version": 4
}
